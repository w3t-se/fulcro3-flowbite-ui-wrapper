{
  "name": "fulcro3-flowbite-ui-wrapper",
  "version": "1.0.0",
  "description": "Flowbite UI Wrapper =================== :toc: :toc-placement: preamble :toc-levels: 2",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3t-se/fulcro3-flowbite-ui-wrapper.git"
  },
  "scripts": {
    "shadow:watch": "shadow-cljs watch app",
    "shadow:release": "shadow-cljs release app",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fulcrologic/semantic-ui-wrapper/issues"
  },
  "homepage": "https://github.com/fulcrologic/semantic-ui-wrapper#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shadow-cljs": "^2.20.16",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "flowbite": "^1.6.2",
    "flowbite-react": "^0.3.7",
    "highlight.js": "^9.12.0",
    "react-grid-layout": "^0.16.6"
  }
}
